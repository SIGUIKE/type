{
    "name": "sikessem/type",
    "description": "Customize Object-Oriented PHP Variable Types",
    "license": "MIT",
    "keywords": [
        "type",
        "let var"
    ],
    "homepage": "https://packagist.com/packages/sikessem/type",
    "support": {
        "docs": "https://github.com/sikessem/type#readme",
        "source": "https://github.com/sikessem/type",
        "wiki": "https://github.com/sikessem/type/wiki",
        "issues": "https://github.com/sikessem/type/issues",
        "chat": "https://github.com/sikessem/type/discussions"
    },
    "authors": [{
        "name": "SIGUI Kess√© Emmanuel",
        "email": "ske@sikessem.com",
        "homepage": "https://sikessem.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.0.22"
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Sikessem\\Type\\": "src/"
        },
        "files": [
            "inc/type.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "@php vendor/bin/pest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
